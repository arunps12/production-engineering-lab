PREFIX dct:    <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>
PREFIX xsd:    <http://www.w3.org/2001/XMLSchema#>

# Q2: Books published after 2016
SELECT ?title ?year
WHERE {
    ?book a schema:Book ;
          dct:title ?title ;
          schema:datePublished ?year .
    FILTER (?year >= "2017"^^xsd:gYear)
}
ORDER BY ?year
