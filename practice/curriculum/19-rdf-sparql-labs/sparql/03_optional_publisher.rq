PREFIX dct:    <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>

# SPARQL equivalent of SQL Q3: Books with optional publisher (LEFT JOIN)
SELECT ?title ?author ?publisherName
WHERE {
    ?book a schema:Book ;
          dct:title ?title ;
          schema:author ?authorNode .
    ?authorNode schema:name ?author .
    OPTIONAL {
        ?book schema:publisher ?pub .
        ?pub schema:name ?publisherName .
    }
}
ORDER BY ?title
